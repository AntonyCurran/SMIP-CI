    <validationRules>
        <fullName>Consignments_needs_a_schedule</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Type, &quot;Consignment&quot;)  &amp;&amp; ISNULL(Delivery_Schedule__c)</errorConditionFormula>
        <errorDisplayField>Delivery_Schedule__c</errorDisplayField>
        <errorMessage>A consignment must belong to a delivery schedule</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidateOrderStatusForEdit</fullName>
        <active>true</active>
        <errorConditionFormula>if(PRIORVALUE(Status)==&apos;Submitted&apos; &amp;&amp;  ISPICKVAL(Status, &apos;Submitted&apos;), true, false)</errorConditionFormula>
        <errorMessage>Submitted records couldn&apos;t be edited</errorMessage>
    </validationRules>
